# Google Sheets integration
# Server-side environment variables (never prefixed with VITE_)
# Required: service account email with read access to the spreadsheet
GOOGLE_SERVICE_ACCOUNT_EMAIL=

# Required: private key for the service account (use multi-line format or escape newlines with \n)
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY=

# Required: the spreadsheet ID that stores Behaviour360 responses
GOOGLE_SHEETS_SPREADSHEET_ID=

# Optional: override the data range if your sheet uses a different tab or bounds
# GOOGLE_SHEETS_DATA_RANGE=Form Responses 1!A1:Z

# Frontend configuration
# Optional: override the proxy URL if your deployment exposes the API elsewhere
# VITE_SHEETS_PROXY_URL=/api/sheets-data

# Optional: override Sheets column mappings if your survey uses different headers
# VITE_SHEETS_FIELD_MOTIVATION=motivation_score
# VITE_SHEETS_FIELD_ABILITY=ability_score
# VITE_SHEETS_FIELD_DESCRIPTIVENORMS=descriptive_norms
# VITE_SHEETS_FIELD_INJUNCTIVENORMS=injunctive_norms
# VITE_SHEETS_FIELD_SYSTEMREADINESS=system_score
# VITE_SHEETS_FIELD_CURRENTUSE=current_use
# VITE_SHEETS_FIELD_PROMPTFACILITATOR=prompt_facilitator
# VITE_SHEETS_FIELD_PROMPTSPARK=prompt_spark
# VITE_SHEETS_FIELD_PROMPTSIGNAL=prompt_signal
