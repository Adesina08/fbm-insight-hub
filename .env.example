# Google Sheets integration
# GOOGLE_SERVICE_ACCOUNT should contain the full JSON for your service account credentials
GOOGLE_SERVICE_ACCOUNT=

# The spreadsheet ID (the value between /d/ and /edit in the URL)
GOOGLE_SHEETS_ID=

# The A1 range that includes the header row and all submission rows (e.g. Responses!A:Z)
GOOGLE_SHEETS_DATA_RANGE=

# Frontend configuration
# Optional: override the proxy base path if your deployment mounts the API elsewhere
# VITE_KOBO_PROXY_BASE_URL=/api/kobo

# Optional: override column mappings if your sheet uses different question names
# VITE_KOBO_FIELD_MOTIVATION=motivation_score
# VITE_KOBO_FIELD_ABILITY=ability_score
# VITE_KOBO_FIELD_DESCRIPTIVENORMS=descriptive_norms
# VITE_KOBO_FIELD_INJUNCTIVENORMS=injunctive_norms
# VITE_KOBO_FIELD_SYSTEMREADINESS=system_score
# VITE_KOBO_FIELD_CURRENTUSE=current_use
# VITE_KOBO_FIELD_PROMPTFACILITATOR=prompt_facilitator
# VITE_KOBO_FIELD_PROMPTSPARK=prompt_spark
# VITE_KOBO_FIELD_PROMPTSIGNAL=prompt_signal
