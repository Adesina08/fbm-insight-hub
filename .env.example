# Google Sheets integration
# GOOGLE_SERVICE_ACCOUNT should contain the full JSON for your service account credentials
GOOGLE_SERVICE_ACCOUNT=

# The spreadsheet ID (the value between /d/ and /edit in the URL)
GOOGLE_SHEETS_ID=

# The A1 range that includes the header row and all submission rows (e.g. Responses!A:Z)
GOOGLE_SHEETS_DATA_RANGE=

# Frontend configuration
# Optional: override the API base path if your deployment mounts the endpoints elsewhere
# VITE_SHEETS_API_BASE_URL=/api/sheets
# VITE_SHEETS_DATA_URL=/custom/data
# VITE_SHEETS_METADATA_URL=/custom/metadata

# Optional: override column mappings if your sheet uses different question names
# VITE_SHEETS_FIELD_MOTIVATION=motivation_score
# VITE_SHEETS_FIELD_ABILITY=ability_score
# VITE_SHEETS_FIELD_DESCRIPTIVENORMS=descriptive_norms
# VITE_SHEETS_FIELD_INJUNCTIVENORMS=injunctive_norms
# VITE_SHEETS_FIELD_SYSTEMREADINESS=system_score
# VITE_SHEETS_FIELD_CURRENTUSE=current_use
# VITE_SHEETS_FIELD_PROMPTFACILITATOR=prompt_facilitator
# VITE_SHEETS_FIELD_PROMPTSPARK=prompt_spark
# VITE_SHEETS_FIELD_PROMPTSIGNAL=prompt_signal
